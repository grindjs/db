export function seed(db) {
	const table = '{{ table }}'
	const now = db.fn.now()

	return Promise.all([
		// Deletes ALL existing entries
		db(table).del(),

		// Inserts seed entries
		db(table).insert([
			{
				id: 1,
				name: 'One',
				created_at: now,
				updated_at: now
			}, {
				id: 2,
				name: 'Two',
				created_at: now,
				updated_at: now
			}, {
				id: 3,
				name: 'Three',
				created_at: now,
				updated_at: now
			}
		])
	])
}
